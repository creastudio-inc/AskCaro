
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>AskCaro - Admin Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/admin/assets/images/favicon.ico">

    @RenderSection("styles", false)


    <!-- App css -->
    <link href="~/admin/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/admin/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/admin/assets/css/app.min.css" rel="stylesheet" type="text/css" />

</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">



                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle nav-user mr-0 waves-effect waves-light" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <img src="~/admin/assets/images/users/avatar-4.jpg" alt="user-image" class="rounded-circle">
                        <span class="pro-user-name ml-1">
                            Agnes K <i class="mdi mdi-chevron-down"></i>
                        </span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                        <!-- item-->
                        <div class="dropdown-item noti-title">
                            <h6 class="m-0">
                                Welcome !
                            </h6>
                        </div>

                        <!-- item-->
                        <a href="javascript:void(0);" class="dropdown-item notify-item">
                            <i class="dripicons-user"></i>
                            <span>My Account</span>
                        </a>

                        <!-- item-->
                        <a href="javascript:void(0);" class="dropdown-item notify-item">
                            <i class="dripicons-gear"></i>
                            <span>Settings</span>
                        </a>


                        <div class="dropdown-divider"></div>

                        <!-- item-->
                        <a href="javascript:void(0);" class="dropdown-item notify-item">
                            <i class="dripicons-power"></i>
                            <span>Logout</span>
                        </a>

                    </div>
                </li>


            </ul>

            <ul class="list-unstyled menu-left mb-0">
                <li class="float-left">
                    <a href="index.html" class="logo">
                        <span class="logo-lg">
                            <img src="~/admin/assets/images/logo-light.png" alt="" height="22">
                        </span>
                        <span class="logo-sm">
                            <img src="~/admin/assets/images/logo-sm.png" alt="" height="24">
                        </span>
                    </a>
                </li>
                <li class="float-left">
                    <a class="button-menu-mobile navbar-toggle">
                        <div class="lines">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </a>
                </li>
            </ul>
        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left-side-menu">

            <div class="slimscroll-menu">

                <!--- Sidemenu -->
                <div id="sidebar-menu">

                    <ul class="metismenu" id="side-menu">

                        <li class="menu-title">Navigation</li>

                        <li>
                            <a href="@Url.Action("Index","Home")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","Home","active")">
                                <i class="dripicons-store"></i>
                                <span> Dashboard </span>
                            </a>
                        </li>


                        <li class="menu-title">Aspirateur</li>

                        <li>
                            <a href="@Url.Action("Index","StackOverflow")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","Home","active")">
                                <i class="dripicons-pin"></i>
                                <span> StackOverflow </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","StackExchange")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","StackExchange","active")">
                                <i class="dripicons-toggles"></i>
                                <span> StackExchange </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","ServerFault")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","ServerFault","active")">
                                <i class="dripicons-bookmarks"></i>
                                <span> ServerFault </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","CodeProject")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","CodeProject","active")">
                                <i class="dripicons-view-thumb"></i>
                                <span> CodeProject </span>
                            </a>
                        </li>
                        <li class="menu-title">Setting</li>

                        <li>
                            <a href="@Url.Action("Index","Setting")" class="@AskCaro.MenuHelper.IsSelected(this.Html,"Index","Setting","active")">
                                <i class="dripicons-stack"></i>
                                <span> Setting </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Translate")" class=" @AskCaro.MenuHelper.IsSelected(this.Html,"Index","Translate","active")">
                                <i class="dripicons-pencil"></i>
                                <span> Translate </span>
                            </a>
                        </li>
                       
                    </ul>

                </div>
                <!-- End Sidebar -->

                <div class="clearfix"></div>

            </div>
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">

                <!-- Start Content-->
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box">
                                @*<div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Greeva</a></li>
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Pages</a></li>
                                        <li class="breadcrumb-item active">Starter</li>
                                    </ol>
                                </div>*@
                                <h4 class="page-title">@ViewBag.title</h4>
                            </div>
                        </div>
                    </div>
                    <!-- end page title -->
                    @RenderBody()
                </div> <!-- container-fluid -->

            </div> <!-- content -->
            <!-- Footer Start -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12 text-center">
                            2018 - 2019
                        </div>
                    </div>
                </div>
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    <!-- Vendor js -->
    <script src="~/admin/assets/js/vendor.min.js"></script>
    @RenderSection("scripts", false)

    <!-- App js -->
    <script src="~/admin/assets/js/app.min.js"></script>

</body>
</html>