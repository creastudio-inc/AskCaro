
<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="img\xfavicon.png.pagespeed.ic.WlJ-a2FcKI.png" type="image/x-icon">
    <title>AskCaro</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Caro/StyleSheet.min.css">

    <!--icon font css-->








    <link rel="stylesheet" href="~/Caro/css/style.css">
    <link rel="stylesheet" href="~/Caro/css/responsive.css">
</head>

<body>
    <div id="preloader">
        <div id="ctn-preloader" class="ctn-preloader">
            <div class="animation-preloader">
                <div class="spinner"></div>
                <div class="txt-loading">
                    <span data-text-preloader="A" class="letters-loading">
                       A
                    </span>
                    <span data-text-preloader="S" class="letters-loading">
                        S
                    </span>
                    <span data-text-preloader="K" class="letters-loading">
                        K
                    </span>
                    <span data-text-preloader=" " class="letters-loading">
                         
                    </span>
                    <span data-text-preloader="C" class="letters-loading">
                        C
                    </span>
                    <span data-text-preloader="A" class="letters-loading">
                        A
                    </span>
                    <span data-text-preloader="R" class="letters-loading">
                        R
                    </span>
                    <span data-text-preloader="O" class="letters-loading">
                        O
                    </span>
                </div>
                <p class="text-center">Loading</p>
            </div>
            <div class="loader">
                <div class="row">
                    <div class="col-3 loader-section section-left">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-left">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-right">
                        <div class="bg"></div>
                    </div>
                    <div class="col-3 loader-section section-right">
                        <div class="bg"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="body_wrapper">
        @RenderBody()
        <footer class="new_footer_area bg_color">
            <div class="new_footer_top">
                 <div class="footer_bg">
                    <div class="footer_bg_one"></div>
                    <div class="footer_bg_two"></div>
                </div>
            </div>
            <div class="footer_bottom">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-6 col-sm-7">
                            <p class="mb-0 f_400">© SaasLand Inc.. 2019 All rights reserved.</p>
                        </div>
                        <div class="col-lg-6 col-sm-5 text-right">
                            <p>Made with <i class="icon_heart"></i> in <a href="#">Me</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="~/Caro/js/jquery-3.2.1.min.js"></script>
    <script src="~/Caro/js/propper.js"></script>
    <script src="~/Caro/js/bootstrap.min.js"></script>
    <script src="~/Caro/vendors/bootstrap-selector/js/bootstrap-select.min.js"></script>
    <script src="~/Caro/vendors/wow/wow.min.js"></script>
    <script src="~/Caro/vendors/sckroller/jquery.parallax-scroll.js"></script>
    <script src="~/Caro/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Caro/vendors/nice-select/jquery.nice-select.min.js"></script>
    <script src="~/Caro/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/Caro/vendors/isotope/isotope-min.js"></script>
    <script src="~/Caro/vendors/magnify-pop/jquery.magnific-popup.min.js"></script>
    <script src="~/Caro/vendors/circle-progress/circle-progress.js"></script>
    <script src="~/Caro/vendors/counterup/jquery.counterup.min.js"></script>
    <script src="~/Caro/vendors/counterup/jquery.waypoints.min.js"></script>
    <script src="~/Caro/vendors/counterup/appear.js"></script>
    <script src="~/Caro/vendors/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Caro/js/plugins.js"></script>
    <script src="~/Caro/vendors/multiscroll/jquery.easings.min.js"></script>
    <script src="~/Caro/vendors/multiscroll/multiscroll.responsiveExpand.limited.min.js"></script>
    <script src="~/Caro/vendors/multiscroll/jquery.multiscroll.extensions.min.js"></script>
    <script src="~/Caro/js/main.js"></script>

</body>

</html>